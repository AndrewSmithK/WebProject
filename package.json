{
  "name": "qover-static",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "npm run build",
    "dev": "concurrently --kill-others \"npm run dev-api\" \"npm run dev-server\" \"npm run webpack-dev-server\"",
    "dev-api": "NODE_ENV=development nodemon --exec babel-node ./bin/api.js",
    "dev-server": "NODE_ENV=development nodemon --exec babel-node ./bin/server.js",
    "webpack-dev-server": "NODE_ENV=development babel-node ./bin/webpack-dev-server.js",
    "start": "concurrently --kill-others \"npm run start-api\" \"npm run start-server\"",
    "start-api": "NODE_ENV=production babel-node ./bin/api.js",
    "start-server": "NODE_ENV=production babel-node ./bin/server.js",
    "start-forever": "concurrently forever stopall \"npm run start-api-forever\" \"npm run start-server-forever\"",
    "start-api-forever": "NODE_ENV=production forever start -c node_modules/.bin/babel-node ./bin/api.js",
    "start-server-forever": "NODE_ENV=production forever start -c node_modules/.bin/babel-node ./bin/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Volodymyr Kravchuk <volodymyr.krvchuk@inveritasoft.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "nodemon": "^1.10.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bootstrap": "^4.0.0-alpha.6",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "http-proxy": "^1.15.1",
    "material-ui": "^0.19.0",
    "node-sass": "^3.7.0",
    "normalize-scss": "^7.0.0",
    "postcss-loader": "^0.13.0",
    "postcss-scss": "^1.0.2",
    "pretty-error": "^2.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-isomorphic-tools": "^2.5.8"
  }
}
